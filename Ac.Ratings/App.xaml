<Application
    x:Class="Ac.Ratings.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Ac.Ratings"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/TextBoxTheme.xaml" />
                <ResourceDictionary Source="Theme/TextBlockTheme.xaml" />
                <ResourceDictionary Source="Theme/ModernSlider.xaml" />
                <ResourceDictionary Source="Theme/ModernCheckbox.xaml" />
                <ResourceDictionary Source="Theme/ModernButton.xaml" />
                <ResourceDictionary Source="Theme/SecondarySlider.xaml" />
                <ResourceDictionary Source="Theme/ModernComboBox.xaml" />
                <ResourceDictionary Source="Theme/PrimaryListView.xaml" />
                <ResourceDictionary Source="Theme/Scroll.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
